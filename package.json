{
  "name": "securable.client",
  "version": "0.5.0",
  "description": "Frontend Client for dashboard.securable.io",
  "main": "index.js",
  "scripts": {
    "production": "RAILS_ENV=production npm run build:webpack",
    "development": "RAILS_ENV=development npm start",
    "meta:watch:webpack": "nodemon --config ./.app.nodemon.json --exec npm run watch:webpack",
    "meta:watch:dll": "nodemon --config ./.vendor.nodemon.json --exec npm run build:dll ",
    "meta:watch": "npm run clean && npm run build:dll && npm run meta:watch:webpack ",
    "start": "npm run build",
    "test": "RAILS_ENV=testing ./node_modules/.bin/karma start",
    "clean": "rm -rf /usr/src/app/public/bassets",
    "clean:all": "npm run kill:zombies && npm run clean:dll && npm run clean",
    "clean:dll": "(rm /usr/src/app/client/dll/**.json || echo 'dll folder not found'); (rm -rf /usr/src/app/public/dll-assets || echo 'dll manifests not found')",
    "build:webpack": "webpack --config ./config/app/build.js",
    "build:dll": "webpack --config ./config/vendor/build.js",
    "build": "npm run clean && npm run build:dll && npm run build:webpack",
    "watch:webpack": "NODE_WATCH=true npm run build:webpack",
    "watch": "npm run clean && npm run build:dll && npm run watch:webpack",
    "monitor:dll": "webpack-dashboard -p 3002",
    "monitor:webpack": "webpack-dashboard -p 3001",
    "zombies": "ps aux | grep -E 'nodemon|webpack|node' | grep -v grep | awk '{print $2}'",
    "kill:zombies": "kill -9 $(npm run zombies --silent) && echo 'All zombies terminated' || echo 'No zombies are around'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-stage-3": "^6.22.0",
    "browserslist": "https://registry.npmjs.org/browserslist/-/browserslist-1.7.7.tgz",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "happypack": "^3.0.3",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.3",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "postcss": "^5.2.17",
    "postcss-loader": "^1.3.3",
    "postcss-modules": "^0.6.4",
    "pug": "^2.0.0-beta11",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "uglify-js": "^2.8.21",
    "uglifyjs": "^2.4.10",
    "uglifyjs-webpack-plugin": "^0.4.0",
    "vue-loader": "^12.0.3",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.5.1",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-merge": "^4.1.0",
    "webpack-stats-plugin": "^0.1.5"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "chart.js": "^2.6.0",
    "element-ui": "^1.3.1",
    "es6-promise": "^4.1.0",
    "font-awesome": "^4.7.0",
    "inputmask": "^3.3.5",
    "jquery": "^2.2.4",
    "jquery-circliful": "^0.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "papaparse": "^4.3.3",
    "raphael": "^2.2.7",
    "vue": "^2.3.3",
    "vue-chartjs": "^2.6.3",
    "vue-the-mask": "^0.4.0",
    "whatwg-fetch": "^2.0.3"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "browserslist": {
    "production": [
      "ie >= 9",
      "last 2 versions"
    ],
    "development": [
      "last 2 versions"
    ]
  }
}
